{
	"servers": {
		"context7": {
			"command": "npx",
			"args": [
				"-y",
				"@upstash/context7-mcp@latest"
			],
			"type": "stdio"
		},
		"filesystem": {
			"command": "npx",
			"args": [
				"-y",
				"@modelcontextprotocol/server-filesystem@latest",
				"/home/hukai/main/wattle/services/wattle-scheduler"
			],
			"type": "stdio"
		},
		"rust-docs": {
			"command": "npx",
			"args": [
				"-y",
				"@upstash/context7-mcp@latest"
			],
			"type": "stdio",
			"env": {
				"CONTEXT7_LIBRARY": "rust-lang/rust"
			}
		},
		"cargo-docs": {
			"command": "npx",
			"args": [
				"-y",
				"@upstash/context7-mcp@latest"
			],
			"type": "stdio",
			"env": {
				"CONTEXT7_LIBRARY": "rust-lang/cargo"
			}
		},
		"crates-io": {
			"command": "npx",
			"args": [
				"-y",
				"@upstash/context7-mcp@latest"
			],
			"type": "stdio",
			"env": {
				"CONTEXT7_LIBRARY": "crates.io/docs"
			}
		},
		"memory": {
			"command": "npx",
			"args": [
				"-y",
				"@modelcontextprotocol/server-memory@latest"
			],
			"type": "stdio"
		},
		"sequential-thinking": {
			"command": "npx",
			"args": [
				"-y",
				"@modelcontextprotocol/server-sequential-thinking@latest"
			],
			"type": "stdio"
		}
	},
	"inputs": []
}